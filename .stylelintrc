{
    "extends": "stylelint-config-recommended",
    "rules": {
        "at-rule-no-unknown": [
            true,
            {
                "ignoreAtRules": [
                    "extends",
                    "mixin",
                    "use",
                    "include"
                ]
            }
        ],
        "selector-class-pattern": null,
        "block-no-empty": null,
        "selector-type-no-unknown": null,
        "no-invalid-double-slash-comments": null
    }
}